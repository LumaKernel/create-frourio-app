{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "<% if (orm === 'prisma') { %>npm run migrate:dev && <% } %>run-p dev:*",
    "dev:client": "next dev -p <%= clientPort %>",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida<% if (orm === 'prisma') { %> && prisma generate --schema server/prisma/schema.prisma<% } %> && next build<% if (building === 'static') { %> && next export<% } %>",
    "build:server": "npm run build --prefix server",
    "build:types": "aspida && npm run build:frourio --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix<% if (orm === 'prisma') { %>",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server<% } else if (orm === 'typeorm') { %>",
    "migration:generate": "npm run migration:generate --prefix server",
    "migration:run": "npm run migration:run --prefix server",
    "migration:revert": "npm run migration:revert --prefix server<% } %>",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server<% if (testing === 'jest') { %>",
    "test": "npm run build:types && jest<% } %>",
    "typecheck": "npm run build:types && tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "<% if (aspida === 'axios') { %>@aspida/axios": "^1.4.1",
    "<% } else { %>@aspida/fetch": "^1.4.1<% if (testing === 'jest') { %>",
    "@aspida/node-fetch": "^1.4.1<% } %>",
    "<% } %><% if (reactHooks === 'swr') { %>@aspida/swr": "^1.4.1",
    "<% } else if (reactHooks === 'query') { %>@aspida/react-query": "^1.4.1",
    "<% } %><% if (aspida === 'axios') { %>axios": "^0.21.1",
    "<% } %>class-validator": "^0.13.1",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1<% if (reactHooks === 'swr') { %>",
    "swr": "^0.4.1<% } else if (reactHooks === 'query') { %>",
    "react-query": "^3.7.1<% } %>"
  },
  "devDependencies": {
    "<% if (testing === 'jest') { %>@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "<% } %>@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2<% if (testing === 'jest') { %>",
    "babel-jest": "^26.6.3<% } %>",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0<% if (testing === 'jest') { %>",
    "fastify": "^3.11.0",
    "fastify-cors": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3<% } %>",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1<% if (orm === 'prisma') { %>",
    "prisma": "^2.16.0<% } %><% if (testing === 'jest') { %>",
    "ts-jest": "^26.5.0<% } %>",
    "ts-loader": "^8.0.15<% if (testing === 'jest') { %>",
    "ts-node": "^9.1.1<% } %>",
    "typescript": "^4.1.3"
  }
}
